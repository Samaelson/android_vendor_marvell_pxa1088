<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.sec.android.app.launcher">
    <home>

        <!--  screen [0]  -->
        <!-- Weather -->
        <appwidget
            launcher:packageName="com.sec.android.widgetapp.at.hero.accuweather"
            launcher:className="com.sec.android.widgetapp.at.hero.accuweather.WeatherClock"
            launcher:screen="0"
            launcher:x="0"
            launcher:y="0"
            launcher:spanX="6"
            launcher:spanY="3" />

       <!-- Folder : Google -->
        <folder
            launcher:screen="0"
            launcher:title="Google"
            launcher:x="0"
            launcher:y="5" >
	    
            <!-- Gmail -->
            <favorite
                launcher:screen="0"
                launcher:packageName="com.google.android.gm"
                launcher:className="com.google.android.gm.ConversationListActivityGmail" />
            <!-- Google+ -->
            <favorite
                launcher:screen="1"
                launcher:packageName="com.google.android.apps.plus"
                launcher:className="com.google.android.apps.plus.phone.HomeActivity" />
            <!-- Maps -->
            <favorite
                launcher:screen="2"
                launcher:packageName="com.google.android.apps.maps"
                launcher:className="com.google.android.maps.MapsActivity" />
            <!-- Play Music -->
            <favorite
                launcher:screen="3"
                launcher:packageName="com.google.android.music"
                launcher:className="com.android.music.activitymanagement.TopLevelActivity" />
            <!-- Play Movies -->
            <favorite
                launcher:screen="4"
                launcher:packageName="com.google.android.videos"
                launcher:className="com.google.android.youtube.videos.EntryPoint" />
            <!-- Play Books -->
            <favorite
                launcher:screen="5"
                launcher:packageName="com.google.android.apps.books"
                launcher:className="com.google.android.apps.books.app.BooksActivity" />
            <!-- Play Magazines -->
            <favorite
                launcher:screen="6"
                launcher:packageName="com.google.android.apps.magazines"
                launcher:className="com.google.apps.dots.android.app.activity.CurrentsStartActivity" />
            <!-- Play Games -->
            <favorite
                launcher:screen="7"
                launcher:packageName="com.google.android.play.games"
                launcher:className="com.google.android.gms.games.ui.destination.main.MainActivity" />
            <!-- Drive -->
            <favorite
                launcher:screen="8"
                launcher:packageName="com.google.android.apps.docs"
                launcher:className="com.google.android.apps.docs.app.NewMainProxyActivity" />
            <!-- YouTube -->
            <favorite
                launcher:screen="9"
                launcher:packageName="com.google.android.youtube"
                launcher:className="com.google.android.youtube.app.honeycomb.Shell$HomeActivity" />
            <!-- Photos -->
            <favorite
                launcher:screen="10"
                launcher:packageName="com.google.android.apps.plus"
                launcher:className="com.google.android.apps.plus.phone.ConversationListActivity" />
            <!-- Hangouts -->
            <favorite
                launcher:screen="11"
                launcher:packageName="com.google.android.talk"
                launcher:className="com.google.android.talk.SigningInActivity" />
        </folder>
	
        <!-- Chrome -->
        <favorite
            launcher:packageName="com.android.chrome"
            launcher:className="com.google.android.apps.chrome.Main"
            launcher:screen="0"
            launcher:x="1"
            launcher:y="5"
            launcher:spanX="1"
            launcher:spanY="1" />
	    
	<!-- Play Store -->
        <favorite
            launcher:packageName="com.android.vending"
            launcher:className="com.android.vending.AssetBrowserActivity"
            launcher:screen="0"
            launcher:x="2"
            launcher:y="5"
            launcher:spanX="1"
            launcher:spanY="1" />
	    
	 <!-- Contact -->
        <favorite
	    launcher:packageName="com.android.contacts"
            launcher:className="com.android.contacts.activities.PeopleActivity"
            launcher:screen="0"
            launcher:x="3"
            launcher:y="5"
            launcher:spanX="1"
            launcher:spanY="1" />
	    
	<!--Memo-->
	<favorite 
	    launcher:packageName="com.samsung.android.app.memo" 
	    launcher:className="com.samsung.android.app.memo.Main" 
            launcher:screen="0"
            launcher:x="4"
            launcher:y="5"
            launcher:spanX="1"
            launcher:spanY="1" />
	    
	<!--Internet-->
        <favorite
		launcher:packageName="com.sec.android.app.sbrowser" 
		launcher:className="com.sec.android.app.sbrowser.SBrowserMainActivity" 
            launcher:screen="0"
            launcher:x="5"
            launcher:y="5"
            launcher:spanX="1"
            launcher:spanY="1" />
	    
	    
	<!--  screen [1]  -->
	<!--Samsung Apps-->
	<appwidget 
		launcher:packageName="com.sec.android.widgetapp.samsungapps" 
		launcher:className="com.sec.android.widgetapp.samsungapps.WidgetProvider43" 
		launcher:screen="1" 
		launcher:x="0" 
		launcher:y="0" 
		launcher:spanX="6" 
		launcher:spanY="3" />
		    
        <!-- Camera -->
        <favorite
            launcher:packageName="com.sec.android.app.camera"
            launcher:className="com.sec.android.app.camera.Camera"
            launcher:screen="1"
            launcher:x="0"
            launcher:y="5"
            launcher:spanX="1"
            launcher:spanY="1" />

    <!-- Gallery -->
    <favorite
        launcher:packageName="com.sec.android.gallery3d"
        launcher:className="com.sec.android.gallery3d.app.Gallery"
        launcher:screen="1"
        launcher:x="1"
        launcher:y="5"
        launcher:spanX="1"
        launcher:spanY="1" />

    <!--S Planner-->
    <favorite
        launcher:packageName="com.android.calendar"
        launcher:className="com.android.calendar.AllInOneActivity"
        launcher:screen="1"
        launcher:x="2"
        launcher:y="5"
        launcher:spanX="1"
        launcher:spanY="1" />

    <!--Email-->
    <favorite
        launcher:packageName="com.android.email"
        launcher:className="com.android.email.activity.Welcome"
        launcher:screen="1"
        launcher:x="3"
        launcher:y="5"
        launcher:spanX="1"
        launcher:spanY="1" />

    <!-- Kids Mode / sub - Myfiles -->
    <appwidget
        launcher:packageName="com.sec.kidsplat.installer"
        launcher:className="com.sec.kidsplat.installer.KidsModeInstallerWidget"
        launcher:screen="1"
        launcher:x="4"
        launcher:y="5"
        launcher:spanX="1"
        launcher:spanY="1" />
    <favorite
        launcher:packageName_sub="com.sec.android.app.myfiles"
        launcher:className_sub="com.sec.android.app.myfiles.MainActivity"
        launcher:screen="1"
        launcher:x="4"
        launcher:y="5"
        launcher:spanX="1"
        launcher:spanY="1" />

    <!-- Dropbox -->
    <favorite
        launcher:packageName="com.dropbox.android"
        launcher:className="com.dropbox.android.activity.DropboxBrowser"
        launcher:screen="1"
        launcher:x="5"
        launcher:y="5"
        launcher:spanX="1"
        launcher:spanY="1" />


    </home>

